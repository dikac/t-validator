{
  "name": "t-validator",
  "version": "0.0.1",
  "description": "Typescript validator",
  "dependencies": {
    "@types/node": "^12.11.7",
    "file-copy-no-overwrite": "git+https://github.com/dikac/file-copy-no-overwrite.git",
    "npm-run-all": "^4.1.5",
    "t-validatable": "git+https://github.com/dikac/t-validatable.git",
    "typescript": "=3.5.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "jasmine": "^3.4.0"
  },
  "scripts": {
    "spec-run": "jasmine --config=./jasmine.json",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-compile": "npm-run-all --parallel spec-compile-script ",
    "spec-compile-watch": "npm-run-all --parallel spec-compile-watch-script",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "install": "npm-run-all production compile",
    "production": "file-copy-no-overwrite tsconfig-production.json tsconfig.json"
  },
  "files": [
    "tsconfig-development.json",
    "src/",
    ".gitignore",
    "LICENSE",
    "Readme.md",
    "tsconfig-production.json"
  ],
  "author": "",
  "license": "MIT"
}
