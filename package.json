{
  "name": "@dikac/t-validator",
  "version": "0.1.3",
  "description": "Typescript validator",
  "dependencies": {
    "@dikac/t-validatable": "^0.6.5",
    "@types/node": "^12.12.11",
    "@dikac/deployer": "^0.1.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "jasmine": "^3.4.0"
  },
  "scripts": {
    "spec-run": "jasmine --config=./jasmine.json",
    "spec": "npm-run-all spec-compile spec-run",
    "spec-compile": "npm-run-all --parallel spec-compile-script ",
    "spec-compile-watch": "npm-run-all --parallel spec-compile-watch-script",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "postinstall": "npm-run-all deploy",
    "deploy": "deployer /.git /dist /"
  },
  "files": [
    "dist/",
    "LICENSE",
    "Readme.md"
  ],
  "author": "",
  "license": "MIT"
}
